---
import BlogPost from '../components/BlogPost.astro';
import Layout from '../layouts/layout.astro';

import cv from '../data/cv.json';
import MissionList from '../components/MissionList.svelte';

// const cvr = cv.toReversed();
const missions = [...cv].reverse();

import * as myPost from '../content/other/technos.md';
const { Content } = myPost;
---

<Layout>
  <BlogPost
    title='Resume'
    description='Lorem ipsum dolor sit amet'
    heroImage='old_computer.png'
    pubDate={new Date()}>
    <MissionList client:load {missions} />

    <Content />
  </BlogPost>
</Layout>
